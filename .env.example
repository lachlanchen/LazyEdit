# Example environment overrides for LazyEdit
LAZYEDIT_UPLOAD_DIR=/path/to/LazyEdit/DATA
LAZYEDIT_CAPTION_PYTHON=/path/to/miniconda/envs/caption/bin/python
LAZYEDIT_CAPTION_PRIMARY_ROOT=/path/to/vit-gpt2-image-captioning
LAZYEDIT_CAPTION_FALLBACK_SCRIPT=/path/to/image_captioning/clip-gpt-captioning/src/v2c.py
LAZYEDIT_CAPTION_FALLBACK_CWD=/path/to/LazyEdit
LAZYEDIT_AUTOPUBLISH_URL=http://lazyingart:8081/publish

# Whisper/VAD pipeline
LAZYEDIT_WHISPER_SCRIPT=/path/to/LazyEdit/whisper_with_lang_detect/vad_lang_subtitle.py
LAZYEDIT_WHISPER_MODEL=large-v3
LAZYEDIT_WHISPER_FALLBACK_MODEL=large-v2

# Grsai / Veo API
# Host(Global): https://grsaiapi.com
# Host(China): https://grsai.dakka.com.cn
GRSAI_API_BASE=https://grsaiapi.com
GRSAI_API_KEY=YOUR_GRSAI_API_KEY
GRSAI_API_KEY_HEADER=Authorization

# Venice + A2E pipeline
VENICE_API_BASE=https://api.venice.ai/api/v1
VENICE_API_KEY=
VENICE_MODEL=venice-uncensored
VENICE_CHAT_ENDPOINT=/chat/completions
VENICE_TIMEOUT_SECONDS=60

A2E_API_BASE=https://video.a2e.ai
A2E_API_KEY=
A2E_TIMEOUT_SECONDS=60
A2E_POLL_INTERVAL_SECONDS=3
A2E_POLL_TIMEOUT_SECONDS=240
# Optional TTS endpoints for audio generation
A2E_TTS_ENDPOINT=
A2E_TTS_STATUS_ENDPOINT=
# Provide either A2E_TTS_USER_VOICE_ID or A2E_TTS_ID (A2E_TTS_VOICE_ID is legacy)
A2E_TTS_USER_VOICE_ID=
A2E_TTS_ID=
A2E_TTS_VOICE_ID=
# Optional voice list endpoints to auto-pick a default voice
A2E_TTS_VOICE_LIST_ENDPOINTS=/api/v1/tts/most_used,/api/v1/tts/preview/list,/api/v1/anchor/voice_list
